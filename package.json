{
  "name": "@xapi/scorm",
  "version": "0.1.0",
  "description": "Communicate over the xAPI SCORM profile using JavaScript.",
  "module": "dist/SCORM.esm.js",
  "browser": "dist/SCORM.umd.js",
  "typings": "dist/types/SCORM.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "clean": "rimraf ./dist",
    "build:js": "rollup --config",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "test": "jest",
    "test:format": "prettier --check .",
    "lint": "eslint . --ext .ts,.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xapijs/scorm.git"
  },
  "keywords": [
    "xapi",
    "scorm",
    "profile",
    "typescript"
  ],
  "author": "Christian Cook",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xapijs/scorm/issues"
  },
  "homepage": "https://github.com/xapijs/scorm",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.10",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "babel-jest": "^26.5.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.5.3",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@xapi/xapi": "^0.6.9",
    "axios": "^0.21.1",
    "uuid": "^8.3.1"
  }
}
