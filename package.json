{
  "name": "@xapi/scorm",
  "version": "0.0.1",
  "description": "A JavaScript library for communicating with an LRS over the xAPI SCORM profile.",
  "main": "dist/SCORM.js",
  "module": "dist/SCORM.esm.js",
  "typings": "dist/types/SCORM.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "clean": "rimraf ./dist",
    "build:js": "rollup --config",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "test": "jest",
    "lint": "eslint . --ext .ts,.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xapijs/scorm.git"
  },
  "keywords": [
    "xapi",
    "scorm",
    "profile",
    "typescript"
  ],
  "author": "Christian Cook",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xapijs/scorm/issues"
  },
  "homepage": "https://github.com/xapijs/scorm",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "babel-jest": "^25.2.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cross-fetch": "^3.0.4",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "jest": "^25.2.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.7.5",
    "ts-jest": "^25.3.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@xapi/xapi": "file:../xapi",
    "crypto-js": "^4.0.0",
    "uuidv4": "^6.0.8"
  }
}
